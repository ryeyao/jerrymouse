<?xml version = "1.0" encoding = "UTF-8"?>
<wshare xmlns:xsd="http://www.w3.org/2001/XMLSchema-instance"
	xsd:noNamespaceSchemaLocation="F:\wshareData.xsd">
	<device seq="1">
		<check>abcd</check>
		<key>179</key>
		<LocalID>179</LocalID>
		<gateway>192.168.119.254</gateway>
		<description>锚节点</description>

		
		<devicetype name="锚节点">
			<type>锚节点</type>
			<description>锚节点</description>
			<icon>gif/deviceicon/sb.png</icon>
			<property name="manufacture">IIECAS</property>
		</devicetype>

		<datatype name="温度">
			<description>温度数据</description>
			<icon>gif/deviceicon/wd.gif</icon>
			<valuetype>float</valuetype>
			<max_value>-40</max_value>
			<min_value>80</min_value>
			<unit>C</unit>
			<stream>温度流</stream>	
			<command name="设置上报频率" dataid="1">
				<des>打开/关闭数据上报</des>
				<argument type="int">Interval：数据上报的周期，interval为0时，表示关闭数据上报</argument>
				<argument type="short">SequenceNumber：命令的序列号，没发一次命令，序列号加1</argument>
			</command>
		</datatype>
		
		<datatype name="湿度">
			<description>湿度数据</description>
			<icon>gif/deviceicon/sd.gif</icon>
			<valuetype>float</valuetype>
			<max_value>-40</max_value>
			<min_value>80</min_value>
			<unit>C</unit>
			<stream>湿度流</stream>	
			<command name="设置上报频率" dataid="1">
				<des>打开/关闭数据上报</des>
				<argument type="int">Interval：数据上报的周期，interval为0时，表示关闭数据上报</argument>
				<argument type="short">SequenceNumber：命令的序列号，没发一次命令，序列号加1</argument>
			</command>
		</datatype>		
		
		
		
		<datatype name="定位数据">
			<description>定位数据</description>
			<icon>gif/deviceicon/定位.gif</icon>
			<valuetype>[int,int,int]</valuetype>
			<max_value>[65535,65535,65535]</max_value>
			<min_value>[0,-65535,-65535]</min_value>
			<stream>定位数据流流</stream>	
		</datatype>
	</device>
</wshare>

